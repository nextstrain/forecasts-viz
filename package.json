{
  "name": "@nextstrain/evofr-viz",
  "version": "0.1.0",
  "description": "React components for visualising evofr model JSONs",
  "private": true,
  "author": "Nextstrain",
  "license": "AGPL-3.0-only",
  "repository": "github:nextstrain/forecasts-viz",
  "main": "dist/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "scripts/build.sh",
    "test": "react-scripts test",
    "lint": "eslint src",
    "eject": "react-scripts eject",
    "docs": "jsdoc2md src/lib/index.js src/lib/**/*.js > api.md",
    "transpile": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "prepack": "npm run transpile"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "files": [
    "dist",
    "api.md",
    "README.md"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "d3": "7.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "argparse": "^2.0.1",
    "express": "^4.18.2",
    "jsdoc": "^4.0.0",
    "jsdoc-to-markdown": "^8.0.0",
    "puppeteer": "^19.4.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  }
}
